<style>
    table, th, td {
        border: 1px solid black;
    }
</style>
<h2>
    Students list
</h2>
<table>

    <tr>
        <th>
            id
        </th>
        <th>
            email
        </th>
        <th>
            modifier
        </th>
    </tr>
    {% for student in students %}
    <tr>
        <td>
            {{ student.id }}
        </td>
        <td>
            {{ student.email }}
        </td>
    <td>
        <a href="{{ path('Studentupdate',{'id':student.id} ) }}">modifier</a>
        <a href="{{ path('deletestudent',{'id':student.id} ) }}">supprimer</a>
    </td>
    </tr>
    {% endfor %}
</table>